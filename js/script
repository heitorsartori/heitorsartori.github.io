function onOf_saida() {
     var element = document.getElementById("btn-onOf_saida");
     element.classList.toggle("onOf-saida");
 }

function onOf_retorno() {
    var element = document.getElementById("btn-onOf_retorno");
    element.classList.toggle("glyphicon-volume-off");
    element.classList.toggle("glyphicon-volume-up");
}
